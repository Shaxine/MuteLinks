{
    "applications": {
      "gecko": {
        "id": "@mutelinks",
        "strict_min_version": "51.0"
      }
    },

    "developer": {
      "name": "Shaxine",
      "url": "https://addons.mozilla.org/en-US/firefox/user/Shaxine/"
    },

    "background": {
      "scripts": ["background.js"]
    },

    "description": "Mute Tabs based of URL",

    "icons": {
      "48": "icon48.png",
      "96": "icon.png"
    },

    "manifest_version": 2,

    "name": "MuteLinks",

    "options_ui": {
      "page": "options/options.html"
    },

    "permissions": [
      "contextMenus",
      "storage",
      "tabs"
    ],

    "version": "0.2beta",

    "web_accessible_resources": ["images/my-image.png"]
}